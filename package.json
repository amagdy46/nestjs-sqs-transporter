{
  "name": "nestjs-sqs-transporter",
  "version": "0.3.0",
  "description": "NestJS SQS Custom Transporter following official microservice patterns with S3 large message support, DLQ handling, and OpenTelemetry observability",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "biome check src test",
    "lint:fix": "biome check --write src test",
    "format": "biome format --write src test",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "nestjs",
    "sqs",
    "aws",
    "microservices",
    "transporter",
    "message-queue",
    "event-driven"
  ],
  "author": "Prepit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amagdy46/nestjs-sqs-transporter"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.0.0",
    "sqs-consumer": "^12.0.0",
    "sqs-producer": "^7.0.0",
    "uuid": "^11.0.0"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "@nestjs/microservices": "^10.0.0 || ^11.0.0",
    "rxjs": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-s3": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "@opentelemetry/api": "^1.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/client-sqs": "^3.700.0",
    "@biomejs/biome": "^2.3.11",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/microservices": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^4.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
